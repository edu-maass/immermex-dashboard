{
  "version": 2,
  "builds": [
    {
      "src": "main_production.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_production.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12",
    "DEBUG": "false"
  },
  "functions": {
    "main_production.py": {
      "maxDuration": 30
    }
  }
}
