{
  "version": 2,
  "builds": [
    {
      "src": "main_with_db.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_with_db.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "ENVIRONMENT": "production",
    "DEBUG": "false",
    "LOG_LEVEL": "info"
  },
  "functions": {
    "main_with_db.py": {
      "maxDuration": 30
    }
  }
}
