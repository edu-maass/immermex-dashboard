{
  "version": 2,
  "builds": [
    {
      "src": "main_with_db.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_with_db.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "true",
    "LOG_LEVEL": "debug",
    "DATABASE_URL": "sqlite:///./immermex.db"
  }
}