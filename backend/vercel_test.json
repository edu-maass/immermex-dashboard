{
  "version": 2,
  "builds": [
    {
      "src": "main_minimal_test.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_minimal_test.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "test",
    "DEBUG": "true"
  }
}
