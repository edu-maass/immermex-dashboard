{
  "version": 2,
  "builds": [
    {
      "src": "main_with_db.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_with_db.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
