{
  "version": 2,
  "builds": [
    {
      "src": "backend/simple_upload.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/simple_upload.py"
    }
  ],
  "env": {
    "PYTHONPATH": "backend"
  }
}
